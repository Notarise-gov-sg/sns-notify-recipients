{
  "name": "@notarise-gov-sg/sns-notify-recipients",
  "version": "0.0.0-development",
  "description": "Send SNS notifications with templates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Notarise-gov-sg/sns-notify-recipients.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --emitDeclarationOnly && babel src -d dist --ignore src/**/*.spec.ts,src/**/*.test.ts -x .js,.ts,.tsx",
    "preversion": "npm run lint && npm test && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "commitmsg": "commitlint -e",
    "commitlint-circle": "commitlint-circle",
    "upgrade-deps": "npx updtr"
  },
  "keywords": [
    "generator-nod"
  ],
  "dependencies": {
    "@aws-sdk/client-sns": "^3.282.0",
    "@aws-sdk/types": "^3.272.0",
    "aws-sdk": "^2.1330.0",
    "aws-xray-sdk-core": "^3.4.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.1.6",
    "debug": "^4.3.4",
    "nric": "^1.2.2",
    "qrcode": "^1.5.1",
    "runtypes": "^6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt": "^17.4.4",
    "@ls-age/commitlint-circle": "^1.0.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.1",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.1",
    "commitizen": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "git-cz": "^4.9.0",
    "jest": "^27.1.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "open-cli": "^7.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^20.1.1",
    "semantic-release-npm-github-publish": "^1.5.4",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "engines": {
    "node": ">= 10.18"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
