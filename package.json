{
  "name": "@notarise-gov-sg/sns-notify-recipients",
  "version": "0.0.0-development",
  "description": "Send SNS notifications with templates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Notarise-gov-sg/sns-notify-recipients.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --emitDeclarationOnly && babel src -d dist --ignore src/**/*.spec.ts,src/**/*.test.ts -x .js,.ts,.tsx",
    "preversion": "npm run lint && npm test && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "commitmsg": "commitlint -e",
    "commitlint-circle": "commitlint-circle",
    "upgrade-deps": "npx updtr",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --ext js,ts,tsx",
      "git add"
    ]
  },
  "keywords": [
    "generator-nod"
  ],
  "dependencies": {
    "aws-sdk": "^2.655.0",
    "aws-xray-sdk-core": "3.2.0",
    "date-fns": "^2.21.1",
    "date-fns-tz": "^1.1.4",
    "debug": "4.3.1",
    "qrcode": "^1.4.4",
    "runtypes": "5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "7.14.0",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@commitlint/prompt": "12.1.1",
    "@ls-age/commitlint-circle": "1.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.2",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.23",
    "@types/qrcode": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.3",
    "eslint": "7.25.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "git-cz": "4.7.6",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "open-cli": "6.0.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "semantic-release-npm-github-publish": "1.4.0",
    "typescript": "4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "engines": {
    "node": ">= 10.18"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
